<Simulation verbosity="debug">
  <TestInfo>
    <name>framework.sept_milestone</name>
    <author>mandd, wangc</author>
    <created>2019-05-14</created>
    <classesTested>Models.EnsembleModel</classesTested>
    <description>
       This test is aimed to check the craft workflow for Sept. 2018 milestone
    </description>
  </TestInfo>

  <RunInfo>
      <WorkingDir>MFW</WorkingDir>
      <Sequence>MultiRun,statPP</Sequence>
      <batchSize>1</batchSize>
  </RunInfo>

  <Steps>
      <MultiRun  name='MultiRun'>
        <Input   class="DataObjects" type="PointSet"      >inputPlaceHolderMain</Input>
        <Model   class='Models'      type='ExternalModel' >CRAFT_Model</Model>
        <Sampler class='Samplers'    type='MonteCarlo'    >MC_main</Sampler>
        <Output  class='DataObjects' type='DataSet'       >main</Output>
        <Output  class='DataObjects' type='HistorySet'     >outHS</Output>
      </MultiRun>
      <IOStep name='print'>
        <Input   class="DataObjects" type="DataSet"       >main</Input>
        <Output  class='OutStreams'  type='Print'         >Print_sim_PS</Output>
      </IOStep>
      <PostProcess name="statPP">
        <Input class="DataObjects" type="HistorySet">outHS</Input>
        <Model class="Models" type="PostProcessor">statAnalysis</Model>
        <Output class="DataObjects" type="HistorySet">stat_HS</Output>
        <Output class="OutStreams" type="Print">Print_basicStats</Output>
        <Output class="OutStreams" type="Plot">plot_V1</Output>
        <Output class="OutStreams" type="Plot">plot_V2</Output>
        <Output class="OutStreams" type="Plot">plot_SG</Output>
        <Output class="OutStreams" type="Plot">plot_P1P2_1</Output>
        <Output class="OutStreams" type="Plot">plot_P1P2_2</Output>
        <Output class="OutStreams" type="Plot">plot_cost_V1</Output>
        <Output class="OutStreams" type="Plot">plot_cost_V2</Output>
        <Output class="OutStreams" type="Plot">plot_cost_SG</Output>
        <Output class="OutStreams" type="Plot">plot_cost_P1P2_1</Output>
        <Output class="OutStreams" type="Plot">plot_cost_P1P2_2</Output>
      </PostProcess>
  </Steps>

  <Distributions>
      <Normal name='b_V1_dist'>
        <mean>2.5E-9</mean>
        <sigma>2.E-9</sigma>
        <lowerBound>-2.0E-9</lowerBound>
        <upperBound>5.E-8</upperBound>
      </Normal>
      <Normal name='b_V2_dist'>
        <mean>3.75E-5</mean>
        <sigma>1.E-4</sigma>
      </Normal>
      <Uniform name='alpha_P1_dist'>
        <lowerBound>2.0</lowerBound>
        <upperBound>2.5</upperBound>
      </Uniform>
      <Uniform name='beta_P1_dist'>
        <lowerBound>0.2</lowerBound>
        <upperBound>0.3</upperBound>
      </Uniform>
      <Uniform name='delta_P2_dist'>
        <lowerBound>0.1</lowerBound>
        <upperBound>0.6</upperBound>
      </Uniform>
      <Uniform name='alpha_SG_dist'>
        <lowerBound>60.</lowerBound>
        <upperBound>70.</upperBound>
      </Uniform>
      <Uniform name='beta_SG_dist'>
        <lowerBound>10.</lowerBound>
        <upperBound>15.</upperBound>
      </Uniform>
      <!-- Distributions for Cost Model: cost_P1P2.py-->
      <Uniform name='P1P2_numberDaysSD_dist'>
        <lowerBound>7.0</lowerBound>
        <upperBound>21.0</upperBound>
      </Uniform>
      <!--
        Based on 300MW per hour, 11.4 cents per KWH (based on average price of US during
        1998 ~ 2017). Variation of prices is 0.8 cents per KWH
           -->
      <Uniform name='P1P2_costPerDaySD_dist'>
        <lowerBound>7.92E5</lowerBound>
        <upperBound>8.50E5</upperBound>
      </Uniform>
      <Uniform name='P1P2_numberDaysPred_dist'>
        <lowerBound>2.0</lowerBound>
        <upperBound>7.0</upperBound>
      </Uniform>
      <!--
        Based on 300MW per hour, 11.4 cents per KWH (based on average price of US during
        1998 ~ 2017). Variation of prices is 0.8 cents per KWH. Power reduce to 70%
           -->
      <Uniform name='P1P2_costPerDayPred_dist'>
        <lowerBound>2.376E5</lowerBound>
        <upperBound>2.550E5</upperBound>
      </Uniform>
      <Uniform name='P1P2_costPerDayReg_dist'>
        <lowerBound>2000</lowerBound>
        <upperBound>3000</upperBound>
      </Uniform>
      <!-- Distributions for Cost Model: cost_SG.py-->
      <Uniform name='SG_numberDaysSD_dist'>
        <lowerBound>30.0</lowerBound>
        <upperBound>60.0</upperBound>
      </Uniform>
      <Uniform name='SG_costPerDaySD_dist'>
        <lowerBound>7.92E5</lowerBound>
        <upperBound>8.50E5</upperBound>
      </Uniform>
      <!-- Distributions for Cost Model: cost_V1.py-->
      <Uniform name='V1_numberDaysSD_dist'>
        <lowerBound>10.0</lowerBound>
        <upperBound>30.0</upperBound>
      </Uniform>
      <Uniform name='V1_costPerDaySD_dist'>
        <lowerBound>7.92E5</lowerBound>
        <upperBound>8.50E5</upperBound>
      </Uniform>
      <!-- Distributions for Cost Model: cost_V2.py-->
      <Uniform name='V2_numberDaysSD_dist'>
        <lowerBound>10.0</lowerBound>
        <upperBound>30.0</upperBound>
      </Uniform>
      <Uniform name='V2_costPerDaySD_dist'>
        <lowerBound>7.92E5</lowerBound>
        <upperBound>8.50E5</upperBound>
      </Uniform>
  </Distributions>

  <Samplers>
    <MonteCarlo name="MC_main">
      <samplerInit>
        <limit>10</limit>
        <initialSeed>2561978</initialSeed>
      </samplerInit>

      <constant name='a_V1'>1.5E-5</constant>
      <variable name='b_V1'>
        <distribution>b_V1_dist</distribution>
      </variable>

      <constant name='a_V2'>-5.</constant>
      <variable name='b_V2'>
        <distribution>b_V2_dist</distribution>
      </variable>

      <variable name='alpha_P1'>
        <distribution>alpha_P1_dist</distribution>
      </variable>
      <variable name='beta_P1'>
        <distribution>beta_P1_dist</distribution>
      </variable>

      <variable name='delta_P2'>
        <distribution>delta_P2_dist</distribution>
      </variable>

      <variable name='alpha_SG'>
        <distribution>alpha_SG_dist</distribution>
      </variable>
      <variable name='beta_SG'>
        <distribution>beta_SG_dist</distribution>
      </variable>
      <!-- Variables for Cost Models -->
      <!-- Variables for Cost Model: cost_P1P2.py -->
      <variable name='P1P2_numberDaysPred'>
        <distribution>P1P2_numberDaysPred_dist</distribution>
      </variable>
      <variable name='P1P2_costPerDayPred'>
        <distribution>P1P2_costPerDayPred_dist</distribution>
      </variable>
      <variable name='P1P2_numberDaysSD'>
        <distribution>P1P2_numberDaysSD_dist</distribution>
      </variable>
      <variable name='P1P2_costPerDaySD'>
        <distribution>P1P2_costPerDaySD_dist</distribution>
      </variable>
      <variable name='P1P2_costPerDayReg'>
        <distribution>P1P2_costPerDayReg_dist</distribution>
      </variable>
      <!-- Variables for Cost Model: cost_SG.py -->
      <variable name='SG_numberDaysSD'>
        <distribution>SG_numberDaysSD_dist</distribution>
      </variable>
      <variable name='SG_costPerDaySD'>
        <distribution>SG_costPerDaySD_dist</distribution>
      </variable>
      <!-- Variables for Cost Model: cost_V1.py -->
      <variable name='V1_numberDaysSD'>
        <distribution>V1_numberDaysSD_dist</distribution>
      </variable>
      <variable name='V1_costPerDaySD'>
        <distribution>V1_costPerDaySD_dist</distribution>
      </variable>
      <!-- Variables for Cost Model: cost_V2.py -->
      <variable name='V2_numberDaysSD'>
        <distribution>V2_numberDaysSD_dist</distribution>
      </variable>
      <variable name='V2_costPerDaySD'>
        <distribution>V2_costPerDaySD_dist</distribution>
      </variable>
    </MonteCarlo>
  </Samplers>

  <Models>
    <EnsembleModel name="CRAFT_Model" subType="">
      <Model class="Models" type="ExternalModel">plantData
        <Input            class="DataObjects" type="PointSet"   >inputPlaceHolderMain</Input>
        <TargetEvaluation class="DataObjects" type="HistorySet" >plantData_PS</TargetEvaluation>
      </Model>
      <Model class="Models" type="ExternalModel">priceData
        <Input            class="DataObjects" type="PointSet"   >inputPlaceHolder_priceData_PS</Input>
        <TargetEvaluation class="DataObjects" type="DataSet"    >priceData_PS</TargetEvaluation>
      </Model>

      <Model class="Models" type="ExternalModel">lambdaV1
        <Input            class="DataObjects" type="PointSet"  >inputPlaceHolder_lambdaV1_PS</Input>
        <TargetEvaluation class="DataObjects" type="DataSet"   >lambdaV1_PS</TargetEvaluation>
      </Model>
      <Model class="Models" type="ExternalModel">lambdaV2
        <Input            class="DataObjects" type="PointSet"  >inputPlaceHolder_lambdaV2_PS</Input>
        <TargetEvaluation class="DataObjects" type="DataSet"   >lambdaV2_PS</TargetEvaluation>
      </Model>
      <Model class="Models" type="ExternalModel">RUL_P1
        <Input            class="DataObjects" type="PointSet"  >inputPlaceHolder_RUL_P1_PS</Input>
        <TargetEvaluation class="DataObjects" type="DataSet"   >RUL_P1_PS</TargetEvaluation>
      </Model>
      <Model class="Models" type="ExternalModel">unav_P2
        <Input            class="DataObjects" type="PointSet"  >inputPlaceHolder_unav_P2_PS</Input>
        <TargetEvaluation class="DataObjects" type="DataSet"   >unav_P2_PS</TargetEvaluation>
      </Model>
      <Model class="Models" type="ExternalModel">RUL_SG
        <Input            class="DataObjects" type="PointSet"  >inputPlaceHolder_RUL_SG_PS</Input>
        <TargetEvaluation class="DataObjects" type="DataSet"   >RUL_SG_PS</TargetEvaluation>
      </Model>

      <Model class="Models" type="ExternalModel">ET_V1
        <Input            class="DataObjects" type="PointSet"  >inputPlaceHolder_ET_V1_PS</Input>
        <TargetEvaluation class="DataObjects" type="DataSet"   >ET_V1_PS</TargetEvaluation>
      </Model>
      <Model class="Models" type="ExternalModel">ET_V2
        <Input            class="DataObjects" type="PointSet"  >inputPlaceHolder_ET_V2_PS</Input>
        <TargetEvaluation class="DataObjects" type="DataSet"   >ET_V2_PS</TargetEvaluation>
      </Model>
      <Model class="Models" type="ExternalModel">ET_SG
        <Input            class="DataObjects" type="PointSet"  >inputPlaceHolder_ET_SG_PS</Input>
        <TargetEvaluation class="DataObjects" type="DataSet"   >ET_SG_PS</TargetEvaluation>
      </Model>
      <Model class="Models" type="ExternalModel">ET_P1P2
        <Input            class="DataObjects" type="PointSet"  >inputPlaceHolder_ET_P1P2_PS</Input>
        <TargetEvaluation class="DataObjects" type="DataSet"   >ET_P1P2_PS</TargetEvaluation>
      </Model>

      <Model class="Models" type="ExternalModel">cost_V1
        <Input            class="DataObjects" type="PointSet"  >inputPlaceHolder_cost_V1_PS</Input>
        <TargetEvaluation class="DataObjects" type="DataSet"   >cost_V1_PS</TargetEvaluation>
      </Model>
      <Model class="Models" type="ExternalModel">cost_V2
        <Input            class="DataObjects" type="PointSet"  >inputPlaceHolder_cost_V2_PS</Input>
        <TargetEvaluation class="DataObjects" type="DataSet"   >cost_V2_PS</TargetEvaluation>
      </Model>
      <Model class="Models" type="ExternalModel">cost_SG
        <Input            class="DataObjects" type="PointSet"  >inputPlaceHolder_cost_SG_PS</Input>
        <TargetEvaluation class="DataObjects" type="DataSet"   >cost_SG_PS</TargetEvaluation>
      </Model>
      <Model class="Models" type="ExternalModel">cost_P1P2
        <Input            class="DataObjects" type="PointSet"  >inputPlaceHolder_cost_P1P2_PS</Input>
        <TargetEvaluation class="DataObjects" type="DataSet"   >cost_P1P2_PS</TargetEvaluation>
      </Model>
    </EnsembleModel>

    <ExternalModel name="plantData" ModuleToLoad="plantData" subType="">
      <variables>a_V1, b_V1,
                 a_V2, b_V2,
                 alpha_P1, beta_P1,
                 delta_P2,
                 alpha_SG, beta_SG,
                 time,opPower</variables>
    </ExternalModel>

    <ExternalModel name="priceData" ModuleToLoad="priceData" subType="">
      <variables>
          time,opPower,costFactor
      </variables>
    </ExternalModel>
    <!-- Reliability models-->
    <ExternalModel name="lambdaV1" ModuleToLoad="lambdaV1" subType="">
      <variables>opPower, time, a_V1, b_V1,
                 a_V2, b_V2,
                 alpha_P1, beta_P1,
                 delta_P2,
                 alpha_SG, beta_SG,
                 p_V1</variables>
    </ExternalModel>
    <ExternalModel name="lambdaV2" ModuleToLoad="lambdaV2" subType="">
      <variables>opPower, time, a_V1, b_V1,
                 a_V2, b_V2,
                 alpha_P1, beta_P1,
                 delta_P2,
                 alpha_SG, beta_SG,
                 p_V2</variables>
    </ExternalModel>
    <ExternalModel name="RUL_P1" ModuleToLoad="RUL_P1" subType="">
      <variables>opPower, time, a_V1, b_V1,
                 a_V2, b_V2,
                 alpha_P1, beta_P1,
                 delta_P2,
                 alpha_SG, beta_SG,
                 p_P1</variables>
    </ExternalModel>
    <ExternalModel name="unav_P2" ModuleToLoad="unav_P2" subType="">
      <variables>opPower, time, a_V1, b_V1,
                 a_V2, b_V2,
                 alpha_P1, beta_P1,
                 delta_P2,
                 alpha_SG, beta_SG,
                 p_P2</variables>
    </ExternalModel>
    <ExternalModel name="RUL_SG" ModuleToLoad="RUL_SG" subType="">
      <variables>time, opPower,
                 a_V1, b_V1,
                 a_V2, b_V2,
                 alpha_P1, beta_P1,
                 delta_P2,
                 alpha_SG, beta_SG,
                 p_SG</variables>
    </ExternalModel>

    <!-- Performance models-->
    <ExternalModel name="ET_V1" ModuleToLoad="ET_V1" subType="">
      <variables>opPower, time, p_V1, p_V2, p_P1, p_P2, p_SG,
                 outcome_V1</variables>
    </ExternalModel>
    <ExternalModel name="ET_V2" ModuleToLoad="ET_V2" subType="">
      <variables>opPower, time, p_V1, p_V2, p_P1, p_P2, p_SG,
                 outcome_V2</variables>
    </ExternalModel>
    <ExternalModel name="ET_SG" ModuleToLoad="ET_SG" subType="">
      <variables>opPower, time, p_V1, p_V2, p_P1, p_P2, p_SG,
                 outcome_SG</variables>
    </ExternalModel>
    <ExternalModel name="ET_P1P2" ModuleToLoad="ET_P1P2" subType="">
      <variables>opPower, time, p_V1, p_V2, p_P1, p_P2, p_SG,
                 outcome_P1P2_0, outcome_P1P2_1, outcome_P1P2_2</variables>
    </ExternalModel>

    <!-- Cost models-->
    <ExternalModel name="cost_V1" ModuleToLoad="cost_V1" subType="">
      <variables>costFactor, V1_numberDaysSD, V1_costPerDaySD, opPower, time, cost_V1</variables>
    </ExternalModel>
    <ExternalModel name="cost_V2" ModuleToLoad="cost_V2" subType="">
      <variables>costFactor, V2_numberDaysSD, V2_costPerDaySD, opPower, time, cost_V2</variables>
    </ExternalModel>
    <ExternalModel name="cost_SG" ModuleToLoad="cost_SG" subType="">
      <variables>costFactor, SG_numberDaysSD, SG_costPerDaySD, opPower, time, cost_SG</variables>
    </ExternalModel>
    <ExternalModel name="cost_P1P2" ModuleToLoad="cost_P1P2" subType="">
        <variables>
            P1P2_numberDaysSD, P1P2_costPerDayPred, P1P2_numberDaysPred,
            P1P2_costPerDaySD, P1P2_costPerDayReg,opPower, time, cost_P1P2_0,
            cost_P1P2_1, cost_P1P2_2, costFactor
        </variables>
    </ExternalModel>
    <PostProcessor name="statAnalysis" subType="BasicStatistics" verbosity="debug">
      <percentile prefix="percentile" percent="5">statFeatures</percentile>
      <percentile prefix="percentile" percent="25">statFeatures</percentile>
      <percentile prefix="percentile" percent="75">statFeatures</percentile>
      <percentile prefix="percentile" percent="95">statFeatures</percentile>
      <expectedValue prefix="mean">statFeatures</expectedValue>
      <sigma prefix="sigma">statFeatures</sigma>
      <pivotParameter>time</pivotParameter>
    </PostProcessor>
  </Models>

  <DataObjects>
    <HistorySet name="stat_HS">
      <Output>statOutputs</Output>
        <options>
            <pivotParameter>time</pivotParameter>
        </options>
    </HistorySet>
    <HistorySet name="plantData_PS">
      <Input>a_V1, b_V1,
             a_V2, b_V2,
             alpha_P1, beta_P1,
             delta_P2,
             alpha_SG, beta_SG
      </Input>
      <Output>opPower</Output>
        <options>
            <pivotParameter>time</pivotParameter>
        </options>
    </HistorySet>
    <HistorySet name="outHS">
      <Output>
              outcome_V1, outcome_V2, outcome_SG, outcome_P1P2_0, outcome_P1P2_1, outcome_P1P2_2,
              cost_V1, cost_V2, cost_SG, cost_P1P2_0, cost_P1P2_1, cost_P1P2_2
      </Output>
      <options>
          <pivotParameter>time</pivotParameter>
      </options>
    </HistorySet>

    <DataSet name="priceData_PS">
      <Input>
            opPower, time
      </Input>
      <Output>costFactor</Output>
      <Index var="time">opPower,costFactor</Index>
    </DataSet>

    <DataSet name="lambdaV1_PS">
      <Input>opPower, time, a_V1, b_V1,
             a_V2, b_V2,
             alpha_P1, beta_P1,
             delta_P2,
             alpha_SG, beta_SG
      </Input>
      <Output>p_V1</Output>
      <Index var="time">opPower,p_V1</Index>
    </DataSet>
    <DataSet name="lambdaV2_PS">
      <Input>opPower, time, a_V1, b_V1,
             a_V2, b_V2,
             alpha_P1, beta_P1,
             delta_P2,
             alpha_SG, beta_SG
      </Input>
      <Output>p_V2</Output>
      <Index var="time">opPower,p_V2</Index>
    </DataSet>
    <DataSet name="RUL_P1_PS">
      <Input>opPower, time, a_V1, b_V1,
             a_V2, b_V2,
             alpha_P1, beta_P1,
             delta_P2,
             alpha_SG, beta_SG
      </Input>
      <Output>p_P1</Output>
      <Index var="time">opPower,p_P1</Index>
    </DataSet>
    <DataSet name="unav_P2_PS">
      <Input>opPower, time, a_V1, b_V1,
             a_V2, b_V2,
             alpha_P1, beta_P1,
             delta_P2,
             alpha_SG, beta_SG
      </Input>
      <Output>p_P2</Output>
      <Index var="time">opPower, p_P2</Index>
    </DataSet>
    <DataSet name="RUL_SG_PS">
      <Input>opPower, a_V1, b_V1,
             a_V2, b_V2,
             alpha_P1, beta_P1,
             delta_P2,
             alpha_SG, beta_SG
      </Input>
      <Output>p_SG</Output>
      <Index var="time">opPower,p_SG</Index>
    </DataSet>

    <DataSet name="ET_V1_PS">
      <Input>opPower, time, p_V1, p_V2, p_SG, p_P1, p_P2</Input>
      <Output>outcome_V1</Output>
      <Index var="time">opPower,outcome_V1, p_V1, p_V2, p_SG, p_P1, p_P2</Index>
    </DataSet>
    <DataSet name="ET_V2_PS">
       <Input>opPower, time, p_V1, p_V2, p_SG, p_P1, p_P2</Input>
      <Output>outcome_V2</Output>
      <Index var="time">opPower,outcome_V2, p_V1, p_V2, p_SG, p_P1, p_P2</Index>
    </DataSet>
    <DataSet name="ET_SG_PS">
       <Input>opPower, time, p_V1, p_V2, p_SG, p_P1, p_P2</Input>
      <Output>outcome_SG</Output>
      <Index var="time">opPower,outcome_SG, p_V1, p_V2, p_SG, p_P1, p_P2</Index>
    </DataSet>
    <DataSet name="ET_P1P2_PS">
       <Input>opPower, time, p_V1, p_V2, p_SG, p_P1, p_P2</Input>
      <Output>outcome_P1P2_0, outcome_P1P2_1, outcome_P1P2_2</Output>
      <Index var="time">opPower,outcome_P1P2_0, outcome_P1P2_1, outcome_P1P2_2, p_V1, p_V2, p_SG, p_P1, p_P2</Index>
    </DataSet>

    <DataSet name="cost_V1_PS">
        <Input>
            opPower, time, outcome_V1, outcome_V2, outcome_SG, outcome_P1P2_0, outcome_P1P2_1, outcome_P1P2_2,
            V1_numberDaysSD, V1_costPerDaySD, costFactor
        </Input>
      <Output>cost_V1</Output>
      <Index var="time">opPower,cost_V1, outcome_V1, outcome_V2, outcome_SG, outcome_P1P2_0, outcome_P1P2_1, outcome_P1P2_2, costFactor</Index>
    </DataSet>
    <DataSet name="cost_V2_PS">
        <Input>
            opPower, time, outcome_V1, outcome_V2, outcome_SG, outcome_P1P2_0, outcome_P1P2_1, outcome_P1P2_2,
            V2_numberDaysSD, V2_costPerDaySD, costFactor
        </Input>
      <Output>cost_V2</Output>
      <Index var="time">opPower, cost_V2, outcome_V1, outcome_V2, outcome_SG, outcome_P1P2_0, outcome_P1P2_1, outcome_P1P2_2, costFactor</Index>
    </DataSet>
    <DataSet name="cost_SG_PS">
        <Input>
            opPower, time, outcome_V1, outcome_V2, outcome_SG, outcome_P1P2_0, outcome_P1P2_1, outcome_P1P2_2,
            SG_numberDaysSD, SG_costPerDaySD, costFactor
        </Input>
      <Output>cost_SG</Output>
      <Index var="time">opPower, cost_SG, outcome_V1, outcome_V2, outcome_SG, outcome_P1P2_0, outcome_P1P2_1, outcome_P1P2_2, costFactor</Index>
    </DataSet>
    <DataSet name="cost_P1P2_PS">
        <Input>
            opPower, time, outcome_V1, outcome_V2, outcome_SG, outcome_P1P2_0, outcome_P1P2_1, outcome_P1P2_2,
            P1P2_numberDaysPred, P1P2_costPerDayPred, P1P2_numberDaysSD, P1P2_costPerDaySD, P1P2_costPerDayReg, costFactor
        </Input>
      <Output>cost_P1P2_0, cost_P1P2_1, cost_P1P2_2</Output>
      <Index var="time">
          opPower,cost_P1P2_0, cost_P1P2_1, cost_P1P2_2, outcome_V1, outcome_V2, outcome_SG, outcome_P1P2_0, outcome_P1P2_1, outcome_P1P2_2, costFactor
      </Index>
    </DataSet>
    <DataSet name="main">
      <Input>a_V1, b_V1,
             a_V2, b_V2,
             alpha_P1, beta_P1,
             delta_P2,
             alpha_SG, beta_SG,
             P1P2_numberDaysPred, P1P2_costPerDayPred, P1P2_numberDaysSD, P1P2_costPerDaySD, P1P2_costPerDayReg,
             V2_numberDaysSD, V2_costPerDaySD,
             V1_numberDaysSD, V1_costPerDaySD,
             SG_numberDaysSD, SG_costPerDaySD
      </Input>
      <Output>opPower,
              p_V1, p_V2, p_P1, p_P2, p_SG,
              outcome_V1, outcome_V2, outcome_SG, outcome_P1P2_0, outcome_P1P2_1, outcome_P1P2_2,
              cost_V1, cost_V2, cost_SG, cost_P1P2_0, cost_P1P2_1, cost_P1P2_2, costFactor
      </Output>
