<Run_settings>
	<sensitivity>True/False</sensitivity>
	<mode>static/dynamic</mode>
	<limit>20</limit>
	<WorkingDir>.</WorkingDir>
</Run_settings>

<Model> <!-- List of MCSs or path sets generated by PRA codes -->
	<MCS_file>mcsList.csv</MCS_file>
	<top_event_ID>TOP</top_event_ID>
</Model>

<Basic_event name='BE1'>   <!-- for each basic event we can specify either its margin in terms of point value or a pdf-->
	<value>[]</value>
	<distribution type='dist_type'>
		<param1>[]</param1>
		<param2>[]</param2>
	</distribution>
</Basic_event>

<Basic_event name='BE2'>
	<value>[]</value>
	<distribution type='dist_type'>
		<param1>[]</param1>
		<param2>[]</param2>
	</distribution>
</Basic_event>

<Variables>
	<HP_heater_BE1>
		<Uniform>0.05, 0.15</Uniform>
	</HP_heater_BE1>
</Variables>
